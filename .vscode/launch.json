{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "chrome",
      "request": "launch",
      "name": "Launch Chrome against localhost",
      "url": "https://en.wikipedia.org/wiki/China",
      "webRoot": "${workspaceFolder}",
      "sourceMaps": true,
      "skipFiles": ["<node_internals>/**"],
      "runtimeArgs": [
        "--disable-extensions-except=${workspaceFolder}",
        "--load-extension=${workspaceFolder}"
      ]
    }
  ]
}
