{
    "manifest_version": 3,
    "name": "My Extension",
    "version": "1.0",
    "permissions": [
      "activeTab",
      "scripting",
      "storage"
    ],
    "background": {
      "service_worker": "background.js",
      "type": "module"
    },
    "web_accessible_resources": [
      {
        "resources": ["d3.v5.min.js","popup.js","content.css"],
        "matches": ["<all_urls>"]
      }
    ],
    "content_scripts": [
      {
        "matches": ["https://en.wikipedia.org/*"],
        "js": ["content.js","d3.v5.min.js","popup.js"],
        "css": ["content.css"]
      }
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    }
  }
  